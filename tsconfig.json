{
    "files" : [
        "./main.ts",
        "./common.ts",
        "database_rpcs.ts",
        "player_rpcs.ts",
        "server_rpcs.ts"
    ],
    "compilerOptions": {
        "target": "es5",
        "typeRoots": [
            "./node_modules"
        ],
        "types": [
            "nakama-runtime"
        ],
        "outFile": "./build/index.js",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}